{
  "name": "@sankeyangshu/eslint-config",
  "type": "module",
  "version": "0.1.0",
  "packageManager": "pnpm@9.14.2",
  "description": "ESLint config for @sankeyangshu.",
  "author": {
    "name": "sankeyangshu",
    "email": "sankeyangshu@gmail.com",
    "url": "https://github.com/sankeyangshu"
  },
  "license": "MIT",
  "homepage": "https://github.com/sankeyangshu/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sankeyangshu/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/sankeyangshu/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-config-vue",
    "eslint-config-react",
    "eslint-config-react-native",
    "eslint-config-solid",
    "eslint-config-svelte",
    "eslint-config-astro",
    "prettier"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  },
  "scripts": {
    "build": "tsup",
    "build:inspector": "eslint-config-inspector build --config eslint-inspector.config.ts",
    "dev": "eslint-config-inspector --config eslint-inspector.config.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "simple-git-hooks",
    "release": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "watch": "tsup --format esm,cjs --watch"
  },
  "peerDependencies": {
    "@unocss/eslint-plugin": ">=0.64.1",
    "eslint": ">=9.5.0",
    "eslint-plugin-astro": ">=0.30.0",
    "eslint-plugin-react": ">=7.0.0",
    "eslint-plugin-react-hooks": ">=4.0.0",
    "eslint-plugin-react-native": ">=4.0.0",
    "eslint-plugin-react-refresh": ">=0.4.0",
    "eslint-plugin-solid": ">=0.10.0",
    "eslint-plugin-svelte": ">=2.0.0",
    "eslint-plugin-vue": ">=9.19.0",
    "prettier-plugin-astro": ">=0.12.0",
    "prettier-plugin-svelte": ">=3.0.0",
    "prettier-plugin-toml": ">=2.0.0",
    "typescript": ">=5.0.0",
    "vue-eslint-parser": ">=9.3.2"
  },
  "peerDependenciesMeta": {
    "@toml-tools/parser": {
      "optional": true
    },
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-astro": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-native": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "eslint-plugin-solid": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "prettier-plugin-astro": {
      "optional": true
    },
    "prettier-plugin-svelte": {
      "optional": true
    },
    "prettier-plugin-toml": {
      "optional": true
    },
    "vue-eslint-parser": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/eslint-define-config": "1.23.0-2",
    "@antfu/install-pkg": "^0.5.0",
    "@eslint/js": "^9.16.0",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint-config-flat-gitignore": "^0.3.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-parser-plain": "^0.1.0",
    "eslint-plugin-import-x": "^4.5.0",
    "eslint-plugin-n": "^17.15.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.13.0",
    "local-pkg": "^0.5.1",
    "prettier": "^3.4.2",
    "prettier-plugin-jsdoc": "^1.3.0",
    "prettier-plugin-json-sort": "^0.0.2",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@eslint/config-inspector": "^0.6.0",
    "@toml-tools/parser": "^1.0.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.10.1",
    "@types/prompts": "^2.4.9",
    "@unocss/eslint-plugin": "^0.65.1",
    "astro-eslint-parser": "^1.1.0",
    "bumpp": "^9.9.0",
    "eslint": "^9.16.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-svelte": "^2.46.1",
    "eslint-plugin-vue": "^9.32.0",
    "lint-staged": "^15.2.11",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-svelte": "^3.3.2",
    "prettier-plugin-toml": "^2.0.1",
    "simple-git-hooks": "^2.11.1",
    "svelte-eslint-parser": "^0.43.0",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vue-eslint-parser": "^9.4.3"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "npx commitlint -e"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
