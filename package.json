{
  "name": "@sankeyangshu/eslint-config",
  "version": "0.2.2",
  "type": "module",
  "packageManager": "pnpm@10.6.0",
  "description": "ESLint config for @sankeyangshu.",
  "author": {
    "name": "sankeyangshu",
    "email": "sankeyangshu@gmail.com",
    "url": "https://github.com/sankeyangshu"
  },
  "license": "MIT",
  "homepage": "https://github.com/sankeyangshu/eslint-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sankeyangshu/eslint-config.git"
  },
  "bugs": {
    "url": "https://github.com/sankeyangshu/eslint-config/issues"
  },
  "keywords": [
    "eslint",
    "eslint-config",
    "eslint-flat-config",
    "eslint-config-vue",
    "eslint-config-react",
    "eslint-config-react-native",
    "eslint-config-solid",
    "eslint-config-svelte",
    "eslint-config-astro",
    "prettier"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": "^18.18.0 || >=20.0.0"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "scripts": {
    "dev": "eslint-config-inspector --config eslint.config.js",
    "build": "tsup",
    "build:inspector": "eslint-config-inspector build --config eslint.config.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepack": "nr build",
    "release": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "watch": "tsup --format esm,cjs --watch",
    "prepare": "simple-git-hooks"
  },
  "peerDependencies": {
    "@unocss/eslint-plugin": ">=0.64.1",
    "eslint": ">=9.5.0",
    "eslint-plugin-astro": ">=0.30.0",
    "eslint-plugin-react": ">=7.0.0",
    "eslint-plugin-react-hooks": ">=4.0.0",
    "eslint-plugin-react-native": ">=4.0.0",
    "eslint-plugin-react-refresh": ">=0.4.0",
    "eslint-plugin-solid": ">=0.10.0",
    "eslint-plugin-svelte": ">=2.0.0",
    "eslint-plugin-vue": ">=9.19.0",
    "prettier-plugin-astro": ">=0.12.0",
    "prettier-plugin-svelte": ">=3.0.0",
    "prettier-plugin-toml": ">=2.0.0",
    "typescript": ">=5.0.0",
    "vue-eslint-parser": ">=9.3.2"
  },
  "peerDependenciesMeta": {
    "@toml-tools/parser": {
      "optional": true
    },
    "@unocss/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-astro": {
      "optional": true
    },
    "eslint-plugin-react": {
      "optional": true
    },
    "eslint-plugin-react-hooks": {
      "optional": true
    },
    "eslint-plugin-react-native": {
      "optional": true
    },
    "eslint-plugin-react-refresh": {
      "optional": true
    },
    "eslint-plugin-solid": {
      "optional": true
    },
    "eslint-plugin-svelte": {
      "optional": true
    },
    "eslint-plugin-vue": {
      "optional": true
    },
    "prettier-plugin-astro": {
      "optional": true
    },
    "prettier-plugin-svelte": {
      "optional": true
    },
    "prettier-plugin-toml": {
      "optional": true
    },
    "vue-eslint-parser": {
      "optional": true
    }
  },
  "dependencies": {
    "@antfu/eslint-define-config": "1.23.0-2",
    "@antfu/install-pkg": "^1.0.0",
    "@eslint/js": "^9.21.0",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-config-prettier": "^10.0.2",
    "eslint-flat-config-utils": "^2.0.1",
    "eslint-parser-plain": "^0.1.1",
    "eslint-plugin-import-x": "^4.6.1",
    "eslint-plugin-jsdoc": "^50.6.3",
    "eslint-plugin-jsonc": "^2.19.1",
    "eslint-plugin-n": "^17.16.2",
    "eslint-plugin-perfectionist": "^4.9.0",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-unicorn": "^57.0.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "local-pkg": "^1.1.1",
    "prettier": "^3.5.3",
    "prompts": "^2.4.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.7.1",
    "@commitlint/config-conventional": "^19.7.1",
    "@eslint/config-inspector": "^1.0.2",
    "@toml-tools/parser": "^1.0.0",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^22.13.9",
    "@types/prompts": "^2.4.9",
    "@types/react": "18.3.12",
    "@types/react-dom": "18.3.1",
    "@unocss/eslint-plugin": "^66.0.0",
    "astro-eslint-parser": "^1.2.1",
    "bumpp": "^10.0.3",
    "eslint": "^9.21.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-native": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "eslint-plugin-solid": "^0.14.5",
    "eslint-plugin-svelte": "^3.0.3",
    "eslint-plugin-vue": "^10.0.0",
    "execa": "^9.5.2",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.0",
    "lint-staged": "^15.4.3",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-toml": "^2.0.2",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "simple-git-hooks": "^2.11.1",
    "svelte": "^5.22.5",
    "svelte-eslint-parser": "^1.0.1",
    "tsup": "^8.4.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.8",
    "vue": "^3.5.13",
    "vue-eslint-parser": "^10.1.1"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged",
    "commit-msg": "npx --no-install commitlint --edit $1"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
